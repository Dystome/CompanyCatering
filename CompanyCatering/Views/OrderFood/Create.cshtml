@model CompanyCatering.Models.FoodOrderModel

<div>
    <form asp-action="Create">
        <div class="form-group">
            <label asp-for="EmployeeID">Client</label>
            <select class="form-control" asp-for="EmployeeID"
                    asp-items="@(new SelectList(Model.Employees, "ID", "FullName"))"></select>
        </div>

        <div class="form-group">
            <label asp-for="FoodID">Book</label>
            <select class="form-control" asp-for="FoodID"
                    asp-items="@(new SelectList(Model.FoodItems, "Id", "Title"))"></select>
        </div>

        <div class="form-group">
            <input type="submit" value="Rent" class="btn btn-primary" />
        </div>
    </form>
</div>